datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

model Integrante {
  id     Int    @id @default(autoincrement())
  nombre String
  codigo String
  edad   Int
}

model Todo {
  id        Int     @id @default(autoincrement())
  text      String
  completed Boolean @default(false)
}